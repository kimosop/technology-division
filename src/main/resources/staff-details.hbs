{{#partial "content"}}

<h1>Staff list!</h1>

   {{#if staffs}} <!--if posts exist, then -->
       {{#each staffs }} <!--loop-->
        <p>Current post:</p>
        <p>{{ content }},{{ id }},{{createdAt}} </p>
        <p>{{ department }}</p>
         <!--show-->
       {{/each}}
   {{else}} <!--no posts!-->
    <p>No Staff!</p>
   {{/if}}

<a href="/">Go Back</a>
{{/partial}}


{{> layout.hbs}}